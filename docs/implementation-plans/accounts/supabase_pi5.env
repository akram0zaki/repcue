
# Supabase local instance on Raspberry Pi 5
SUPABASE_URL=http://<your-pi-ip>:54321
SUPABASE_ANON_KEY=<anon-key-from-supabase-start>
SUPABASE_SERVICE_ROLE_KEY=<service-role-key-from-supabase-start>

# Local Postgres (Supabase-managed container)
PGHOST=<your-pi-ip>
PGPORT=54322
PGUSER=postgres
PGPASSWORD=postgres
PGDATABASE=postgres
PGSSLMODE=disable
